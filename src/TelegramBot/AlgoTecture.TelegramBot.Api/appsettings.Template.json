{
  "ConnectionStrings": {
    "AlgoTecturePostgresTelegramAccount": "Host=__DB_HOST__;Port=5432;Database=__DB_TELEGRAM_NAME__;Username=__DB_USER__;Password=__DB_PASS__",
    "Redis": "__REDIS_HOST__:6379"
  },
  "DeepSeek": {
    "ApiKey": "__DEEPSEEK_API_KEY__",
    "BaseUrl": "__DEEPSEEK_BASE_URL__",
    "Model": "__DEEPSEEK_MODEL__"
  },
  "Kestrel": {
    "Endpoints": {
      "Rest": {
        "Url": "http://0.0.0.0:5003",
        "Protocols": "Http1"
      }
    }
  },
  "botf": "__BOTF_TOKEN__?chain_timeout=0:2",
  "GeoAdminSettings": {
    "GeoAdminBaseUrl": "__GEO_ADMIN_BASE_URL__"
  },
  "Rabbit": {
    "Host": "__RABBIT_HOST__",
    "Username": "__RABBIT_USER__",
    "Password": "__RABBIT_PASS__"
  },
  "ApiClients": {
    "GatewayBaseUrl": "__GATEWAY_URL__",
    "Identity": {
      "BasePath": "/identity"
    },
    "User": {
      "BasePath": "/user"
    },
    "Space": {
      "BasePath": "/space"
    },
    "Reservation": {
      "BasePath": "/reservation"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Error",
        "Microsoft.EntityFrameworkCore.Database.Command": "Error",
        "MassTransit": "Error",
        "System.Net.Http": "Information",
        "Refit": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      { "Name": "Seq", "Args": { "serverUrl": "__SEQ_URL__" } }
    ]
  }
}
