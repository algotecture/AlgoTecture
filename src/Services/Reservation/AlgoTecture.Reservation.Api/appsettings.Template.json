{
  "ConnectionStrings": {
    "AlgoTecturePostgresReservation": "Host=__DB_HOST__;Port=5432;Database=__DB_RESERVATION_NAME__;Username=__DB_USER__;Password=__DB_PASS__"
  },
  "Kestrel": {
    "Endpoints": {}
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Error",
        "System": "Error",
        "Microsoft.EntityFrameworkCore.Database.Command": "Error",
        "MassTransit": "Error"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      { "Name": "Seq", "Args": { "serverUrl": "__SEQ_URL__" } }
    ]
  }
}
